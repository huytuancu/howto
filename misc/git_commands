# reset to master branch
git reset --hard origin/master

git add . # add all files updated

# push empty commit
git commit --allow-empty -m "Commit message" # delete --allow-empty if adding files

git pull --rebase
git push origin HEAD:refs/for/master # push to HEAD aka latest on master branch

# useful git config
git config user.name < >
git config user.email < >

git config branch.master.url < >
git config branch.master.pushurl < >
...

# check .git configuration
.git/config # th√≠s file stores important information of repo eg: url
